 "html": "{% set company_bank_details = frappe.db.get_list(\"Bank Account\", filters={\"company\" : doc.company} , fields=[\"*\"]) %}\n{%- from \"templates/print_formats/standard_macros.html\" import add_header,\nrender_field -%}\n\n<!-- Outer container for the print format with some styling -->\n<div class=\"print-format .print-format-preview\" style=\"padding-left: 0px\">\n  <!-- Page Header Section -->\n  <div class=\"page-header\">\n    {{ add_header(0, 1, doc, letter_head, no_letterhead ) }}\n  </div>\n\n  <!-- Page Footer Section -->\n  <!--<div class=\"page-footer\">{{footer}}</div>-->\n\n  <!-- Table for the entire print format -->\n      <table>\n            <!-- Table Header Section -->\n            <thead>\n                <tr>\n                    <th>\n                        <!-- Placeholder for the fixed-position header -->\n                        <div class=\"page-header-space\"></div>\n                    </th>\n                </tr>\n                <tr>\n                        <th style=\"border: 1px solid black; width: 9%; color: black; text-align: center;\">Sl No.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 10.5%; text-align: center;\">MRP</th>\n                        <th style=\"border: 1px solid black; color: black; width: 35%; text-align: center;\">SERIES</th>\n                        <th style=\"border: 1px solid black; color: black; width: 19%; text-align: center;\">SHADE NO.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 15%; text-align: center;\">28</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">30</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">32</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">34</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">36 S</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">38 M</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">40 L</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">42 XL</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">44 XXL</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">QTY</th>\n                </tr>\n            </thead>\n        \n            <!-- Table Body Section -->\n            <tbody>\n                {% for i in range(1, 25) %}\n                {% if i % 17 == 0 %}\n                </tbody>\n                </table>\n                <!-- Placeholder for the fixed-position footer -->\n                <div class=\"page-footer-space\"></div>\n                <!-- Page break -->\n                <div class=\"page-break\"></div>\n                <table>\n                <thead>\n                    <tr>\n                        <th>\n                            <!-- Placeholder for the fixed-position header -->\n                            <div class=\"page-header-space\"></div>\n                        </th>\n                    </tr>\n                    <tr>\n                        <th style=\"border: 1px solid black; width: 9%; color: black; text-align: center;\">Sl No.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 10.5%; text-align: center;\">MRP</th>\n                        <th style=\"border: 1px solid black; color: black; width: 35%; text-align: center;\">SERIES</th>\n                        <th style=\"border: 1px solid black; color: black; width: 19%; text-align: center;\">SHADE NO.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 15%; text-align: center;\">28</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">30</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">32</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">34</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">36 S</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">38 M</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">40 L</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">42 XL</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">44 XXL</th>\n                        <th style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\">QTY</th>\n                    </tr>\n                </thead>\n                <tbody>\n                {% endif %}\n                <tr>\n                    <td style=\"border: 1px solid black; width: 9%; color: black; text-align: center;\">{{ i }}</td>\n                    <td style=\"border: 1px solid black; color: black; width: 10.5%; text-align: center;\"></td>\n                    <td style=\"border: 1px solid black; color: black; width: 35%; text-align: center;\"></td>\n                    <td style=\"border: 1px solid black; color: black; width: 19%; text-align: center;\"></td>\n                    <td style=\"border: 1px solid black; color: black; width: 15%; text-align: center;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                    <td style=\"border: 1px solid black; color: black; text-align: center; width: 25%;\"></td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        \n            <!-- Table Footer Section -->\n            <tfoot>\n                <tr>\n                    <td>\n                        <!-- Placeholder for the fixed-position footer -->\n                        <div class=\"page-footer-space\"></div>\n                    </td>\n                </tr>\n            </tfoot>\n            <!-- Additional TableRow -->\n            <tbody>\n                <tr>\n                    <td style=\"border: 1px solid black; width: 9%; color: black; text-align: left; font-size : 11px; font-weight : 500; letter-spacing : 0px; padding : 3px !important;\" colspan=\"6\">\n                        {% for details in company_bank_details %}\n                            BANK DETAILS : {{doc.company}} {{details.bank}} BANK<br>\n                            A/C NO. {{details.bank_account_no}}, K.H. ROAD BRANCH, IFS CODE : ICIC0001937\n                        {% endfor %}\n                    </td>\n                    <td style=\"border: 1px solid black; color: black; width: 10.5%; text-align: left; padding : 0px !important;\" colspan=\"8\">\n                        <div style = \"display : flex; flex-direction : column;\">\n                            <div style = \"border-bottom : 1px solid black; padding : 1px !important;\">Delivery Note : </div>\n                            <div style = \"padding : 1px !important;\">Remarks</div>\n                        </div>\n                    </td>\n                </tr>\n            </tbody>\n\n        </table>\n</div>",
