 "html": "{% set customer_address = frappe.get_list(doctype=\"Address\", filters={\"link_name\" : doc.customer}, fields=[\"*\"]) %}\n{% set fetch_e_Waybill_Log_data = frappe.db.get_list(\"e-Waybill Log\", filters={\"reference_name\": doc.name}, fields=[\"*\"]) %}\n{%- from \"templates/print_formats/standard_macros.html\" import add_header,\nrender_field -%}\n\n\n{% macro display_data(data) %}\n    {% if data is not none %}\n        {{ data }}\n    {% endif %}\n{% endmacro %}\n<!-- Outer container for the print format with some styling -->\n<div class=\"print-format .print-format-preview\" style=\"padding-left: 0px\">\n  <!-- Page Header Section -->\n  <div class=\"page-header\">\n    {{ add_header(0, 1, doc, letter_head, no_letterhead ) }}\n  </div>\n\n  <!-- Page Footer Section -->\n  <div class=\"page-footer\">{{footer}}</div>\n\n  <!-- Table for the entire print format -->\n  <table>\n    <!-- Table Header Section -->\n    <thead>\n      <tr>\n        <td>\n          <!-- Placeholder for the fixed-position header -->\n          <div class=\"page-header-space\"></div>\n        </td>\n      </tr>\n    </thead>\n\n    <!-- Table Body Section -->\n    <tbody>\n        <!-- Placeholder for the main content of the print format -->\n        <!-- Write your print format code here -->\n        <!--<tr style= \"\">-->\n               <tr style= \"\">\n            <td style = \"border-left : 1px solid black; border-right : 1px solid black; border-bottom :0px; padding-bottom : 0 !important;\">\n                <div style = \"display : flex;\">\n                    <div style = \"width : 355px; border-right : 1px solid black; padding : 10px 0px;\">\n                    <div style=\"display: flex; gap: 43px;\">\n                        <div style=\"font-size:15px;\">\n                            <strong>Buyer:</strong>\n                        </div>\n                         <div style=\"flex-shrink: 0; padding-right:\">\n                            <p style = \"font-size : 12px;\">{{ display_data(doc.customer) }}</p>\n                            <div style=\"width : 330px;\">\n                            {% for data in customer_address %}\n                                <p style = \"font-size : 12px;\">{{display_data(data.address_line1)}}<br> {{display_data(data.pincode)}} {{display_data(data.state)}}</p>\n                            {% endfor %}\n                        </div>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap : 25px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                               GST No :\n                            </div>\n                            <div>\n                                {% for data in customer_address %}\n                                <p style = \"font-size : 12px;\">{{display_data(data.gstin)}}</p>\n                                {% endfor %}\n                             </div>\n                             <div style=\"flex-shrink: 0; font-size:15px\">\n                                State Code :\n                        </div>\n                        <div>\n                            {% for data in customer_address %}\n                            <p style = \"font-size : 12px;\">{{display_data(data.gst_state_number)}}</p>\n                            {% endfor %}\n                        </div>\n                        </div>\n                    <div style=\"display: flex; gap : 37px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                                E-mail :\n                        </div>\n                        <div>\n                            {% for data in customer_address %}\n                            <p style = \"font-size :12px;\">{{display_data(data.email_id)}}</p>\n                            {% endfor %}\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap : 38px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                                Ph No :\n                        </div>\n                        <div>\n                            {% for data in customer_address %}\n                            <p style = \"font-size :12px;\">{{display_data(data.phone)}}</p>\n                            {% endfor %}\n                        </div>\n                         \n\n                    </div>\n                </div>\n                    <div style = \"width : 355px; padding : 10px 5px;\">\n                    <div style=\"display: flex; gap: 50px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                           Delivery Note :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(doc.name)}}</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 99px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                            Dated :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(doc.posting_date)}}</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 19px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                            Terms of Payment :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">15 Days</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 77px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                            Transport :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(doc.transporter)}}</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 67px;\">\n                        <div style=\"flex-shrink: 0; font-size:15px;\">\n                            L R No.&Dt :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(doc.lr_no)}}</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 39px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                            Place of Supply :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(doc.place_of_supply)}}</p>\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 68px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                            Destination :\n                        </div>\n                        <div style=\"flex-shrink: 0;\">\n                            {% for data in customer_address %}\n                            <p style = \"font-size : 12px;\">{{display_data(data.city)}}</p>\n                            {% endfor %}\n                        </div>\n                    </div>\n                    <div style=\"display: flex; gap: 102px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                            Agent :\n                        </div>\n                         {%for sales_person in doc.sales_team %}\n                         <div style=\"flex-shrink: 0;\">\n                            <p style = \"font-size : 12px;\">{{display_data(sales_person.sales_person)}}</p>\n                        </div>\n                        {% endfor %}\n                    </div>\n                    <div style=\"display: flex; gap: 25px;\">\n                        <div style=\"flex-shrink: 0;font-size:15px;\">\n                            E-Way Bill :\n                        </div>\n                         <div style=\"flex-shrink: 0; margin-left: 50px;\">\n                            <p style=\"font-size: 12px;\">\n                                {% for details in fetch_e_Waybill_Log_data %}\n                                    {{ display_data(details.name) }}\n                                {% endfor %}\n                            </p>\n                </div>\n                        \n                    </div>\n                </div>\n                </div>\n            </td>\n        </tr>\n        <!--</tr>-->\n            <table style=\"\">\n                <thead>\n                    <tr style=\"border-bottom: 1px solid black; padding-top:2px;\">\n                        <th style=\"border: 1px solid black; width: 10px; color: black; text-align: center;\">Sr No.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 400px; text-align: center;\">Description of Goods</th>\n                        <th style=\"border: 1px solid black; color: black; width: 50px; text-align: center;\">HSN</th>\n                        <th style=\"border: 1px solid black; color: black; width: 50px; text-align: center;\">Qty.</th>\n                        <th style=\"border: 1px solid black; color: black; width: 70px; text-align: center;\">Unit</th>\n                        <th style=\"border: 1px solid black; color: black; width: 70px; text-align: center;\">Rate</th>\n                        <th style=\"border: 1px solid black; color: black; width: 120px; text-align: center;\">Amount(\u20b9)</th>\n                        <th style=\"border: 1px solid black; color: black; width: 120px; text-align: center;\">Kept</th>\n                        <th style=\"border: 1px solid black; color: black; width: 100px; text-align: center;\">GR</th>\n                    </tr>\n                </thead>\n                 <tbody style=\"\">\n                      \n                      \n                      <tr style=\"height:300px;\">\n                    \n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: center; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black; padding-top:5px; padding-bottom:2px;\">{{ display_data(item_data.idx) }}</p>{% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start;  padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px; padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.description) }} </p>  {% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px; padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.qty) }} </p>{% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.qty) }} </p>{% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.uom) }} </p>{% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.rate) }} </p>{% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.amount) }} </p> {% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.idx) }} </p> {% endfor %}</td>\n                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;padding-bottom:2px;padding-left:2px;\">{{ display_data(item_data.idx) }} </p>{% endfor %}</td>\n                      \n                      </tr>\n                    \n      \n\n        <!-- Add more rows as needed -->\n            </tbody>\n                \n        </table>\n    </tr>\n    <!--<tr>-->\n            \n    <!--    <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:160px;\"> </div>-->\n    <!--</tr>-->\n    <tr>\n        <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:50px; display:flex;align-items:center;\"> \n            <div style=\" display:flex; gap : 25px;\">\n                <div style=\"flex-shrink:0;font-size:12px; display:flex;width:374px; justify-content:end;\">{{display_data(doc.total_qty)}}</div>\n                <div style = \"font-size :12px;\">Sub Total</div>\n                <div style=\"font-size:12px\";>{{display_data(doc.total)}}</div>\n           </div>\n        </div>\n    </tr>\n    <tr>\n         <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:110px; display:flex;align-items:center;\">\n            <!--<div style=\"width:32.5%;;height: inherit; display:flex;flex-direction: column;align-items: end; justify-content: center;\">-->\n            <!--        <div style=\"font-size:12px;\">Add :</div>-->\n            <!--        <div style=\"font-size:12px;\">Add :</div>-->\n            <!--        <div style=\"font-size:12px;\">Add :</div>-->\n            <!--    </div>-->\n                <div style=\"width:65%; border-right : 1px solid black;height: inherit; display:flex;flex-direction: column;justify-content:center;align-items: end; \">\n                     <div style = \" text-align : right; padding-right : 16px;\">\n                        {% for data in doc.taxes %} \n                            <em style = \"color : #736f6f;\">{{display_data(data.description)}}@ {{display_data(data.rate)}}%</em>\n                        {% endfor %}\n                    </div>\n                    <div style=\"text-align: right; padding-right: 16px;\">\n                        {% if doc.rounding_adjustment < 0 %}\n                            <em style=\"color: #736f6f;\">Rounded Off(-)</em>\n                        {% else %}\n                            <em style=\"color: #736f6f;\">Rounded Off(+)</em>\n                        {% endif %}\n                    </div>\n                </div>\n                \n                <div style=\"width:35%; display:flex;flex-direction: column;align-items:start; justify-content: center; padding-left:6px;\">\n                     {% for data in doc.taxes %} \n                        <div style = \"font-size:12px;\">{{display_data(data.tax_amount)}}</div>\n                        {% endfor %}\n                    <div style = \"font-size:12px;\">\n                       {{ display_data(doc.rounding_adjustment) }}\n                    </div>\n                        \n                </div>\n            </div>\n            </tr>\n            <tr>\n            <div style = \"display : flex;\">\n                <div style = \"display : flex; padding : 5px; border-left : 1px solid black; width:370px;\">\n                    <div style = \"\">\n                        <strong>{{display_data(doc.in_words)}}</strong>\n                    </div>\n                </div>\n                <div style = \"font-weight : 500;  border-bottom : 1px solid black;padding : 5px; width:108px;\">\n                       <strong>Grand Total \u20b9</strong> \n                    </div>\n                <div style = \"padding-top : 5px; border-right : 1px solid black; border-left : 1px solid black; border-bottom : 1px solid black; width:260px;\">\n                    <center><strong>{{display_data(doc.grand_total)}}</strong></center>\n                </div>\n               \n            </div>\n             \n        </tr>\n       <tr>\n            <div style = \"padding : 5px; border-left : 1px solid black; border-right : 1px solid black; display:flex; justify-content:end;\">\n                <strong>E. & O.E.</strong>\n            </div>\n        </tr>\n        </tr>\n    </tbody>\n\n    <!-- Table Footer Section -->\n    <tfoot>\n      <tr>\n        <td>\n          <!-- Placeholder for the fixed-position footer -->\n          <div class=\"page-footer-space\"></div>\n        </td>\n      </tr>\n    </tfoot>\n  </table>\n</div>",
