 "html": "{% set customer_address = frappe.get_list(\"Address\", filters={\"address_title\": doc.customer}, fields=[\"*\"]) %}\r{%- from \"templates/print_formats/standard_macros.html\" import add_header,\rrender_field -%}\r\r{% macro display_data(data) %}\r    {% if data is not none %}\r        {{ data }}\r    {% endif %}\r{% endmacro %}\r<!-- Outer container for the print format with some styling -->\r<div class=\"print-format .print-format-preview\" style=\"padding-left: 0px\">\r  <!-- Page Header Section -->\r  <div class=\"page-header\">\r    {{ add_header(0, 1, doc, letter_head, no_letterhead ) }}\r  </div>\r\r  <!-- Page Footer Section -->\r  <div class=\"page-footer\">{{footer}}</div>\r\r  <!-- Table for the entire print format -->\r  <table>\r    <!-- Table Header Section -->\r    <thead>\r      <tr>\r        <td>\r          <!-- Placeholder for the fixed-position header -->\r          <div class=\"page-header-space\"></div>\r        </td>\r      </tr>\r    </thead>\r\r    <!-- Table Body Section -->\r    <tbody>\r        <!-- Placeholder for the main content of the print format -->\r        <!-- Write your print format code here -->\r        <!--<tr style= \"\">-->\r            <td style = \"border-left : 1px solid black; border-right : 1px solid black; border-bottom :0px; padding-bottom : 0 !important;\">\r                <div style = \"display : flex;\">\r                    <div style = \"width : 355px; border-right : 1px solid black; padding : 10px 0px;\">\r                    <div style=\"display: flex; gap: 43px;\">\r                        <div style=\"font-size:15px;\">\r                            <strong>Buyer:</strong>\r                        </div>\r                        <div style=\"flex-shrink: 0; padding-right:\">\r                            <p style = \"font-size : 12px;\">{{ display_data(doc.customer) }}</p>\r                            <div style=\"width : 330px;\">\r                            {% for data in customer_address %}\r                                <p style = \"font-size : 12px;\">{{display_data(data.address_line1)}}<br> {{display_data(data.pincode)}} {{display_data(data.state)}}</p>\r                            {% endfor %}\r                        </div>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap : 25px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                               GST No :\r                            </div>\r                            <div>\r                                {% for data in customer_address %}\r                                <p style = \"font-size : 12px;\">{{display_data(data.gstin)}}</p>\r                             </div>\r                              {% endfor %}\r                             <div style=\"flex-shrink: 0; font-size:15px\">\r                                State Code :\r                        </div>\r                              {%  for data in customer_address%}\r                        <div>\r                            <p style = \"font-size : 12px;\">{{display_data(data.gst_state_number)}}</p>\r                        </div>\r                            {% endfor %}\r                        </div>\r                    <div style=\"display: flex; gap : 37px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                                E-mail :\r                        </div>\r                          {% for data in customer_address %}\r                        <div>\r                            <p style = \"font-size :12px;\">{{display_data(data.email_id)}}</p>\r                        </div>\r                        {% endfor %}\r                    </div>\r                    <div style=\"display: flex; gap : 46px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                                Ph No :\r                        </div>\r                        {% for data in customer_address %}\r                        <div>\r                            <p style = \"font-size :12px;\">{{display_data(data.phone)}}</p>\r                        </div>\r                        {% endfor %}\r\r                    </div>\r                </div>\r                    <div style = \"width : 355px; padding : 10px 5px;\">\r                    <div style=\"display: flex; gap: 50px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                           Delivery Note :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(doc.name)}}</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 77px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                            Dated :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(doc.posting_date)}}</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 5px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                            Terms of Payment :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">15 Days</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 58px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                            Transport :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(doc.transporter)}}</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 50px;\">\r                        <div style=\"flex-shrink: 0; font-size:15px;\">\r                            L R No.&Dt :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">2BAG /05-01</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 23px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                            Place of Supply :\r                        </div>\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(doc.place_of_supply)}}</p>\r                        </div>\r                    </div>\r                    <div style=\"display: flex; gap: 49px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                            Destination :\r                        </div>\r                       {% for data in customer_address %}\r                        <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(data.city)}}</p>\r                        </div>\r                         {% endfor %}\r                    </div>\r                    <div style=\"display: flex; gap: 55px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                            Agent :\r                        </div>\r                        {%for sales_person in doc.sales_team %}\r                         <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\">{{display_data(sales_person.sales_person)}}</p>\r                        </div>\r                        {% endfor %}\r                    </div>\r                    <div style=\"display: flex; gap: 57px;\">\r                        <div style=\"flex-shrink: 0;font-size:15px;\">\r                            E-Way Bill :\r                        </div>\r                         <div style=\"flex-shrink: 0;\">\r                            <p style = \"font-size : 12px;\"></p>\r                        </div>\r                        \r                    </div>\r                </div>\r                </div>\r            </td>\r        <!--</tr>-->\r            <table style=\"\">\r                <thead>\r                    <tr style=\"border-bottom: 1px solid black; padding-top:2px;\">\r                        <th style=\"border: 1px solid black; width: 10px; color: black; text-align: center;\">Sr No.</th>\r                        <th style=\"border: 1px solid black; color: black; width: 400px; text-align: center;\">Description of Goods</th>\r                        <th style=\"border: 1px solid black; color: black; width: 50px; text-align: center;\">HSN</th>\r                        <th style=\"border: 1px solid black; color: black; width: 50px; text-align: center;\">Qty.</th>\r                        <th style=\"border: 1px solid black; color: black; width: 70px; text-align: center;\">Unit</th>\r                        <th style=\"border: 1px solid black; color: black; width: 70px; text-align: center;\">Rate</th>\r                        <th style=\"border: 1px solid black; color: black; width: 120px; text-align: center;\">Amount(\u20b9)</th>\r                        <th style=\"border: 1px solid black; color: black; width: 120px; text-align: center;\">Kept</th>\r                        <th style=\"border: 1px solid black; color: black; width: 100px; text-align: center;\">GR</th>\r                    </tr>\r                </thead>\r                 <tbody style=\"\">\r                      \r                      \r                      <tr style=\"height:300px;\">\r                    \r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: center; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black; padding-top:5px;\">{{ display_data(item_data.idx) }}</p>{% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start;  padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.description) }} </p>  {% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.qty) }} </p>{% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.qty) }} </p>{% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.uom) }} </p>{% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.rate) }} </p>{% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.amount) }} </p> {% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %}<p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.idx) }} </p> {% endfor %}</td>\r                        <td style=\"border: 1px solid black; font-size: 12px; color: black; text-align: start; padding:0 !important;\">{% for item_data in doc.items %} <p style=\"border-bottom:1px solid black;padding-top:5px;\">{{ display_data(item_data.idx) }} </p>{% endfor %}</td>\r                      \r                      </tr>\r                    \r      \r\r        <!-- Add more rows as needed -->\r            </tbody>\r                \r        </table>\r    </tr>\r    <!--<tr>-->\r            \r    <!--    <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:160px;\"> </div>-->\r    <!--</tr>-->\r    <tr>\r        <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:50px; display:flex;align-items:center;\"> \r            <div style=\" display:flex; gap : 25px;\">\r                <div style=\"flex-shrink:0;font-size:12px; display:flex;width:374px; justify-content:end;\">{{display_data(doc.total_qty)}}</div>\r                <div style = \"font-size :12px;\">Sub Total</div>\r                <div style=\"font-size:12px\";>{{display_data(doc.total)}}</div>\r           </div>\r        </div>\r    </tr>\r    <tr>\r         <div style=\"border-top:0px ;border-right: 1px solid black; border-left:1px solid black;border-bottom:1px solid black; height:80px; display:flex;align-items:center;\">\r            <!--<div style=\"width:32.5%;;height: inherit; display:flex;flex-direction: column;align-items: end; justify-content: center;\">-->\r            <!--        <div style=\"font-size:12px;\">Add :</div>-->\r            <!--        <div style=\"font-size:12px;\">Add :</div>-->\r            <!--        <div style=\"font-size:12px;\">Add :</div>-->\r            <!--    </div>-->\r                <div style=\"width:65%; border-right : 1px solid black;height: inherit; display:flex;flex-direction: column;justify-content:center;align-items: end; \">\r                     <div style = \" text-align : right; padding-right : 16px;\">\r                        {% for data in doc.taxes %} \r                            <em style = \"color : #736f6f;\">{{display_data(data.description)}}@ {{display_data(data.rate)}}%</em>\r                        {% endfor %}\r                    </div>\r                    <div style=\"text-align: right; padding-right: 16px;\">\r                        {% if doc.rounding_adjustment < 0 %}\r                            <em style=\"color: #736f6f;\">Rounded Off(-)</em>\r                        {% else %}\r                            <em style=\"color: #736f6f;\">Rounded Off(+)</em>\r                        {% endif %}\r                    </div>\r                </div>\r                \r                <div style=\"width:35%; display:flex;flex-direction: column;align-items:start; justify-content: center; padding-left:6px;\">\r                     {% for data in doc.taxes %} \r                        <div style = \"font-size:12px;\">{{display_data(data.tax_amount)}}</div>\r                        {% endfor %}\r                    <div style = \"font-size:12px;\">\r                       {{ display_data(doc.rounding_adjustment) }}\r                    </div>\r                        \r                </div>\r            </div>\r            </tr>\r            <tr>\r            <div style = \"display : flex;\">\r                <div style = \"display : flex; padding : 5px; border-left : 1px solid black; width:370px;\">\r                    <div style = \"\">\r                        <strong>{{display_data(doc.in_words)}}</strong>\r                    </div>\r                </div>\r                <div style = \"font-weight : 500;  border-bottom : 1px solid black;padding : 5px; width:108px;\">\r                       <strong>Grand Total \u20b9</strong> \r                    </div>\r                <div style = \"padding-top : 5px; border-right : 1px solid black; border-left : 1px solid black; border-bottom : 1px solid black; width:260px;\">\r                    <center><strong>{{display_data(doc.grand_total)}}</strong></center>\r                </div>\r               \r            </div>\r             \r        </tr>\r       <tr>\r            <div style = \"padding : 5px; border-left : 1px solid black; border-right : 1px solid black; display:flex; justify-content:end;\">\r                <strong>{{display_data(doc.terms)}}</strong>\r            </div>\r        </tr>\r        </tr>\r    </tbody>\r\r    <!-- Table Footer Section -->\r    <tfoot>\r      <tr>\r        <td>\r          <!-- Placeholder for the fixed-position footer -->\r          <div class=\"page-footer-space\"></div>\r        </td>\r      </tr>\r    </tfoot>\r  </table>\r</div>",
